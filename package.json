{
  "name": "mofix-golden",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p dev:test dev:ui",
    "dev:test": "tsx golden/scripts/test-runner.ts --loop",
    "dev:ui": "sh -c \"cd apps/web && npm run dev\"",
    "golden:gen": "tsx golden/scripts/golden-gen.ts",
    "golden:run": "tsx golden/scripts/test-runner.ts",
    "golden:auto": "tsx golden/scripts/test-runner.ts --loop",
    "golden:ci": "NODE_ENV=ci tsx golden/scripts/test-runner.ts",
    "golden:diff": "tsx golden/scripts/diff.ts",
    "golden:run:t1": "cross-env ZIP_DIR=golden/zips/T1 npm run golden:run",
    "golden:run:t2": "cross-env ZIP_DIR=golden/zips/T2 npm run golden:run",
    "golden:run:t3": "cross-env ZIP_DIR=golden/zips/T3 npm run golden:run",
    "golden:run:t4": "cross-env ZIP_DIR=golden/zips/T4 npm run golden:run",
    "golden:gen:t4": "tsx golden/scripts/t4-gen.ts",
    "golden:report": "jq -r '.results[] | [.file,.framework,(if .ok then \"OK\" else \"FAIL\" end),.installMs,.runMs,.healthMs,.port,.healthUrl] | @tsv' golden/results/*.json | column -t",
    "golden:clean": "rm -rf golden/zips/T1/*.zip golden/zips/T2/*.zip golden/zips/T3/*.zip golden/zips/T4/*.zip golden/results/*.json || true"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/lab": "^7.0.1-beta.18",
    "@mui/material": "^7.3.4",
    "adm-zip": "^0.5.16",
    "cors": "^2.8.5",
    "cross-spawn": "^7.0.6",
    "express": "^5.1.0",
    "fast-glob": "^3.3.3",
    "file-saver": "^2.0.5",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^6.6.0",
    "fs-extra": "^11.3.2",
    "get-port": "^7.1.0",
    "kill-port": "^2.0.1",
    "notistack": "^3.0.2",
    "picocolors": "^1.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/cross-spawn": "^6.0.6",
    "@types/express": "^5.0.4",
    "@types/file-saver": "^2.0.7",
    "@types/kill-port": "^2.0.3",
    "@types/node": "^24.8.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "cross-env": "^7.0.3",
    "esbuild": "^0.20.2",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
