{
  "name": "@dev-migration-hub/web",
  "version": "1.0.0",
  "description": "Web interface for Dev Migration Hub",
  "main": "dist/server.js",
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:server": "tsc src/server.ts --outDir dist --moduleResolution node --esModuleInterop --target ES2020",
    "build:client": "webpack --mode production",
    "dev": "npm run build:client && npm run dev:server",
    "dev:server": "ts-node --project tsconfig.server.json src/server.ts",
    "dev:client": "webpack serve --mode development",
    "start": "npm run build && node dist/server.js"
  },
  "dependencies": {
    "@dev-migration-hub/engine": "file:../../packages/engine",
    "archiver": "^7.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "^11.3.1",
    "http-proxy-middleware": "^3.0.5",
    "multer": "^1.4.5-lts.1",
    "unzipper": "^0.10.14",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.21",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/unzipper": "^0.10.10",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  }
}
